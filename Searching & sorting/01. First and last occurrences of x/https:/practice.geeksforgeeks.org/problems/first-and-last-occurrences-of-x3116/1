vector<int> find(int arr[], int n , int x )
{
    vector<int> v;
    auto first =  lower_bound(arr,arr+n,x);
    auto second = upper_bound(arr,arr+n,x);
    int element = binary_search(arr,arr+n,x);
    if(!element)
    {
        v.push_back(-1);
        v.push_back(-1);
    }
    else
     {
         v.push_back(first-arr);
         v.push_back(second-1-arr);
     }
    
    return v;
}
